public static string DecimaltoCurrency(Decimal val) 
    {
        if (val==0) 
            return '0.00';
        string str = val.divide(1,2,RoundingMode.HALF_UP).format();
        integer dec = str.lastindexOf('.');
        system.debug('-------DecimaltoCurrency: -str/dec/len------------->'+str + '/'+dec+'/'+str.length());
        if (dec==-1 ) 
            return str;
                      
        if(dec==(str.length()-2))
            return str.substring(0,dec+2)+'0';              
        else 
            return str.substring(0,dec+3);
    }
 public static String convertwoDec(String val)
  {
        Integer dec = val.lastindexOf('.');
        if(dec==-1)
        val=val+'.00%';
        else
        val=val+'%';
        return val;  
   }
   //Return negative value -$amount or ($Amount)
   public static String NegativeFormats(Double Value)
    {
    if (Value < 0)
    {
       //-$Amount
        return '-$'+DecimaltoCurrency(Math.abs(Value));

     //($Amount)  return '($'+DecimaltoCurrency(Math.abs(Value)) + ')';
    }
    else
    {
        return '$'+DecimaltoCurrency(Value);
    }
    }
