public without sharing class Legacy_Ext_SmithTravel   //To run a query as "an administrator"
{

    public List<Property_Smith_Travel_Metric_c__x> SmithTravel_List=new List<Property_Smith_Travel_Metric_c__x>();
    public List<Property_Smith_Travel_Metric_c__x> SmithTravel_List_final=new List<Property_Smith_Travel_Metric_c__x>();
    
      
    public double sumPAR=0.0 ;
    public double sumADR=0.0 ;
    public double sumOCC=0.0 ;
    public integer count=0;
    public double sumPropRmsRev=0.0 ;
    public double sumPropRmsAvail=0.0 ;
    public double sumSTRCompRmsRev=0.0 ;
    public double sumSTRCompRmsAvail=0.0 ;
    public double sumPropRmsSold = 0.0;
    public double sumSTRCompRmsSold = 0.0;
 
    
        
    //Class Constructor
    public Legacy_Ext_SmithTravel(ApexPages.StandardController controller) 
    {

    }
 
    
 
        //Method to fetch object data to display on visualforce page.
    public List<Property_Smith_Travel_Metric_c__x> getEvents() 
    {
        
        try 
        { 
        String AccountID=System.currentPageReference().getParameters().get('id');  
          System.debug('Record Id....>'+AccountID);
        
          string sSite_Id=[select EXT_Site_Id__c from account where id=:AccountId][0].EXT_Site_Id__c;
 System.debug('sSite_Id....>'+sSite_Id);
 
        
         
        SmithTravel_List=[SELECT  Name__c, STR_Month_c__c,STR_RevPAR_Comp_Index_c__c, STR_Comp_ADR_Index_c__c, 
                          STR_Comp_Occ_Index_c__c,Month_Number_c__c, STR_Comp_Rms_Avail_c__c, STR_Comp_Rms_Rev_c__c, 
                          STR_Prop_Rms_Rev_c__c, STR_Prop_Rms_Sold_c__c,STR_Prop_Rms_Avail_c__c,STR_Comp_Rms_Sold_c__c 
                          FROM Property_Smith_Travel_Metric_c__x where SF_Connect_Property_Name_c__c=:sSite_Id ORDER BY Name__c desc, Month_Number_c__c desc limit 12];
       System.debug('SmithTravel_List....>'+SmithTravel_List);
        count = SmithTravel_List.size();
         System.debug('count....>'+count);
        System.debug('List size....>'+SmithTravel_List.size());
        if(SmithTravel_List.size()>0)
        {
            
           //calculating the sum of the column values
           FOR(Integer i=0;i<SmithTravel_List.size();i++)
             {  if(SmithTravel_List[i].STR_RevPAR_Comp_Index_c__c!=null)
                 {
                    //sumPAR=sumPAR+SmithTravel_List[i].STR_RevPAR_Comp_Index_c__c;
                    sumPropRmsRev=sumPropRmsRev+ SmithTravel_List[i].STR_Prop_Rms_Rev_c__c;
                    sumPropRmsAvail=sumPropRmsAvail + SmithTravel_List[i].STR_Prop_Rms_Avail_c__c;
                    sumSTRCompRmsRev=sumSTRCompRmsRev + SmithTravel_List[i].STR_Comp_Rms_Rev_c__c;
                    sumSTRCompRmsAvail=sumSTRCompRmsAvail + SmithTravel_List[i].STR_Comp_Rms_Avail_c__c;
                    }
                 else
                 {
                   SmithTravel_List[i].STR_RevPAR_Comp_Index_c__c=0.0;
                 } 
                                        
                if(SmithTravel_List[i].STR_Comp_ADR_Index_c__c!=null)
                 {
                    //sumADR=sumADR+SmithTravel_List[i].STR_Comp_ADR_Index_c__c;
                    sumPropRmsRev=sumPropRmsRev+ SmithTravel_List[i].STR_Prop_Rms_Rev_c__c;
                    sumPropRmsSold=sumPropRmsSold+ SmithTravel_List[i].STR_Prop_Rms_Sold_c__c;
                    sumSTRCompRmsSold=sumSTRCompRmsSold+ SmithTravel_List[i].STR_Comp_Rms_Sold_c__c;
                    sumSTRCompRmsRev=sumSTRCompRmsRev + SmithTravel_List[i].STR_Comp_Rms_Rev_c__c;

                }
                else
                 {
                     SmithTravel_List[i].STR_Comp_ADR_Index_c__c=0.0;
                 }
                   
                if(SmithTravel_List[i].STR_Comp_Occ_Index_c__c!=null)
                 {
                     //sumOCC=sumOCC+SmithTravel_List[i].STR_Comp_Occ_Index_c__c;
                    sumPropRmsSold=sumPropRmsSold+ SmithTravel_List[i].STR_Prop_Rms_Sold_c__c;
                    sumPropRmsAvail=sumPropRmsAvail + SmithTravel_List[i].STR_Prop_Rms_Avail_c__c;
                    sumSTRCompRmsSold=sumSTRCompRmsSold+ SmithTravel_List[i].STR_Comp_Rms_Sold_c__c;
                    sumSTRCompRmsAvail=sumSTRCompRmsAvail + SmithTravel_List[i].STR_Comp_Rms_Avail_c__c;
         
                 }
                else
                 {
                    SmithTravel_List[i].STR_Comp_Occ_Index_c__c=0.0;  
                 }    
              } 
              
              for(Integer i=(SmithTravel_List.size()-1); i>=0; i--)
              {
              System.debug( 'list data..'+SmithTravel_List[i]);
              SmithTravel_List_final.add(SmithTravel_List.get(i));
              
              }
                          //jz  System.debug('my final list size..'+SmithTravel_List_final.size());
                            
                            

              //SmithTravel_List.sort();
              //sumPAR=((sumPropRmsRev/sumPropRmsAvail)/(sumSTRCompRmsRev/sumSTRCompRmsAvail))*100;
              //sumADR=((sumPropRmsRev/sumPropRmsSold)/(sumSTRCompRmsRev/sumSTRCompRmsSold))*100;
              //sumOCC=((sumPropRmsSold/sumPropRmsAvail)/(sumSTRCompRmsSold/sumSTRCompRmsAvail))*100;
              
              //added on 10/22/2012
             try
              {
              sumPAR=((sumPropRmsRev/sumPropRmsAvail)/(sumSTRCompRmsRev/sumSTRCompRmsAvail))*100;
              }
              catch(Exception e)
              {
              sumPAR =0;
              }
              
              try
              {
              sumADR=((sumPropRmsRev/sumPropRmsSold)/(sumSTRCompRmsRev/sumSTRCompRmsSold))*100;
              }
              catch(Exception e)
              {
              sumADR=0;
              }
              
              try
              {
              sumOCC=((sumPropRmsSold/sumPropRmsAvail)/(sumSTRCompRmsSold/sumSTRCompRmsAvail))*100;
              }
              catch(Exception e)
              {
              sumOCC=0;
              }
             
                     
              

              //sumPAR=sumPAR/count;
              //sumADR=sumADR/count;
              //sumOCC=sumOCC/count; 
              
               
               SmithTravel_List_final.add(new Property_Smith_Travel_Metric_c__x(Name__c='Average',STR_Month_c__c='=',STR_RevPAR_Comp_Index_c__c=sumPAR,
                                                                              STR_Comp_ADR_Index_c__c=sumADR,STR_Comp_Occ_Index_c__c=sumOCC));
              return SmithTravel_List_final;
           
        }
        else
        {
            ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.Info,'Data Not Available.'));        
            return null;
        }   
        } catch (exception ex)
        {
               ApexPages.addMessage(new ApexPages.Message(ApexPages.Severity.ERROR,ex.getMessage()) );        
             return null;
        }
        
    }
    
